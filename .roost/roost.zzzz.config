build:
  type: buildpack
  content:
    image: voter
    tag: $GIT_COMMIT_ID
    path: voter
deploy:
  type: text
  content: |
    #/bin/sh

    HELM_OPTS="--set clusterIP=${ROOST_CLUSTER_IP},tag=${GIT_COMMIT_ID},namespace=${ROOST_NAMESPACE}"


    helm upgrade zzzz-helm-vote helm-vote -n $ROOST_NAMESPACE --create-namespace --install $HELM_OPTS
test:
  - name: voter
    configType: cypress
    configPath: service-test-suite/voter
uninstall:
  type: text
  content: |+
    #!/bin/sh

